x1 = int(input('Введите 1-ю сторону 1-го прямоуг.: '))
y1 = int(input('Введите 2-ю сторону 1-го прямоуг.: '))

x2 = int(input('Введите 1-ю сторону 2-го прямоуг.: '))
y2 = int(input('Введите 2-ю сторону 2-го прямоуг.: '))

x3 = int(input('Введите 1-ю сторону 3-го прямоуг.: '))
y3 = int(input('Введите 2-ю сторону 3-го прямоуг.: '))

s1 = x1*y1
s2 = y2*x2
s3 = x3*y3

print(s1)
print(s2)
print(s3)
