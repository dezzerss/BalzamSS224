# -*- coding: utf-8 -*-
name = str(input( "Введите свое ФИО: " ))

if name != "Иван":
    print("Найс, что не Иван")

age = int( input( "Введите свой возраст: " ))

if age < 16:
    g = 16 - age
    print( "Вы слишком молоды для поступления, подождите еще", g ,"лет")

if age >= 16 and age < 75: 
    print( "Поздравляем вы поступили в ВГУИТ" + "!" )

if age > 75:
    print( "Пожалуйста, введите допустимое значение возраста" )
