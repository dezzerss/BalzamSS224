#-*- coding: utf-8 -*-

a = int(input( "Введите первое число: "))

b = int(input( "Второе: "))

c = int(input( "Третье: "))

if a > b < c:
    print( "Наименьшее число равно: ", + int(b))
elif b > a < c:
    print( "Наименьшее число равно: ", + int(a))
else:
    print( "Наименьшее число равно: ", + int(c))
